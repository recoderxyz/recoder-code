{
  "name": "recoder-code-cli",
  "version": "2.4.4",
  "description": "Recoder Code - AI-powered coding assistant CLI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caelum0x/recoder-code.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "recoder-code": "dist/index.js",
    "recoder": "dist/index.js"
  },
  "scripts": {
    "postinstall": "node src/postinstall.cjs",
    "build": "tsc",
    "start": "node dist/index.js",
    "debug": "node --inspect-brk dist/index.js",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "test:integration": "node test-integration.mjs",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist",
    "src/postinstall.cjs"
  ],
  "config": {
    "sandboxImageUri": "ghcr.io/caelum0x/recoder-code:2.4.1"
  },
